{
    "domain": "weibo.com",
    "pages": [
        {
            "urlMatch":"weibo.com\/.+",
            "defaultStyles": 
            ".MIB_feed .MIB_feed_c { float:right; width:474px; } .MIB_feed li { width:540px; } .MIB_feed li { margin:0 0 12px; padding:10px; }"
            ,
            "actions":[{   
                "actionName": "Home",
                "keyChar": "H",
                "keyCode": "72",
                "actionType": "click",
                "isGlobal": 1,
                "actionMatch": ["a.linklogo"]
            },
            {
                "actionName": "ZoomImage", 
                "keyChar":"Z",
                "keyCode":"90",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["div.feed_img > a:first","div.blogPicOri img.cls"]
            },
            {
                "actionName": "JumpToURL", 
                "keyChar":"V",
                "keyCode":"86",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["a[mt='url']"]
            },
            {
                "actionName": "Rt", 
                "keyChar":"R",
                "keyCode":"82",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["div.rt > a > strong[lang='CD0023']"]
            },
            {   
                "actionName": "Comment", 
                "keyChar":"C",
                "keyCode":"67",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["div.rt > a > strong[lang='CL1004']"]
            },
            {   
                "actionName": "Fav", 
                "keyChar":"F",
                "keyCode":"70",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["div.rt > a > strong[lang='CL1003']"]
            }
          ]
        },{

            "urlMatch":"weibo.com\/.[^\/].+",
            "actions":[
            {   
                "actionName": "Next", 
                "keyChar":"N",
                "keyCode":"78",
                "actionType": "next",
                "isGlobal": 1,
                "actionCss":{"background":"#DEF5F8","box-shadow":"0 5px 5px -3px rgba(0, 0, 0, 0.5)"},
                "actionMatch": ["li.MIB_linedot_l"]
            },
            {   
                "actionName": "Prev", 
                "keyChar":"P",
                "keyCode":"80",
                "actionType": "prev",
                "isGlobal": 1,
                "actionCss":{"background":"#DEF5F8","box-shadow":"0 5px 5px -3px rgba(0, 0, 0, 0.5)"},
                "actionMatch": ["li.MIB_linedot_l"]
            },
            {   
                "actionName": "New", 
                "keyChar":"I",
                "keyCode":"73",
                "actionType": "focus",
                "isGlobal": 1,
                "actionMatch": ["#publish_editor"]
            },
            {   
                "actionName": "Search", 
                "keyChar":"S",
                "keyCode":"83",
                "actionType": "focus",
                "isGlobal": 1,
                "actionMatch": ["#m_keyword"]
            }
        
            ]},
        {
            "urlMatch":"weibo.com\/k\/.+",
