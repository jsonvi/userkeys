{
    "domain": "www.google.com.hk",
    "pages": [
        {
            "urlMatch": "google.com[^\/]*\/.+",
            "actions":[
            {   
                "actionName": "Search", 
                "keyChar":"S",
                "keyCode":"83",
                "actionType": "focus",
                "isGlobal": 1,
                "actionMatch": ["input.lst:first"]
            }
            ,
            {
                "actionName": "NextPage",
                "keyChar":"+",
                "keyCode":"187",
                "actionType": "click",
                "isGlobal": 1,
                "actionMatch": ["#pnnext"]
            },
            {
                "actionName": "JumpToURL", 
                "keyChar":"V",
                "keyCode":"86",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["span.tl > h3.r > a"]
            },
            {   
                "actionName": "Next", 
                "keyChar":"N",
                "keyCode":"78",
                "actionType": "next",
                "isGlobal": 1,
                "actionCss":{"background":"#DEF5F8","box-shadow":"0 5px 5px -3px rgba(0, 0, 0, 0.5)"},
                "actionMatch": ["li.g > div.vsc"]
            },
            {   
                 "actionName": "Prev", 
                "keyChar":"P",
                "keyCode":"80",
                "actionType": "prev",
                "isGlobal": 1,
                "actionCss":{"background":"#DEF5F8","box-shadow":"0 5px 5px -3px rgba(0, 0, 0, 0.5)"},
                "actionMatch": ["li.g > div.vsc"]
            }
                
            ]
            
        }
    ]
}
