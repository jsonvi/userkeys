{
    "domain": "qq.com",
    "pages": [
        {
            "urlMatch":"t.qq.com\/.+",
            "actions":[{   
                "actionName": "Home",
                "actionDesc": "回首页",
                "keyChar": "H",
                "actionType": "click",
                "isGlobal": 1,
                "actionMatch": ["h1#logo > a"]
            },
            {
                "actionName": "Rt", 
                "actionDesc": "转播",
                "keyChar":"R",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["div.funBox > a.relay"]
            },
            {   
            "actionName": "Comment", 
            "actionDesc": "评论",
                "keyChar":"C",
                "actionType": "click",
                "isGlobal": 0,
                "actionMatch": ["div.funBox > a.comt"]
            },

            {   
            "actionName": "Next", 
            "actionDesc": "下一条微博",
                "keyChar":"N",
                "actionType": "next",
                "isGlobal": 1,
                "actionCss":{"background":"#DEF5F8","box-shadow":"0 5px 5px -3px rgba(0, 0, 0, 0.5)"},
                "actionMatch": ["#talkList > li"]
            },
            {   
                "actionName": "Prev", 
                "actionDesc": "上一条微博",
                "keyChar":"P",
                "actionType": "prev",
                "isGlobal": 1,
                "actionCss":{"background":"#DEF5F8","box-shadow":"0 5px 5px -3px rgba(0, 0, 0, 0.5)"},
                "actionMatch": ["#talkList > li"]
            }

          ]
        }
    ]
}

