{
    "name": "User Keys",
    "version": "1.2.1",
    "description": "A user key bindings manager.",
    "author" : "jsonvi@gmail.com",
    "update_url": "https://raw.github.com/jsonvi/userkeys/master/updates.xml",
    "icons": {
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "background_page": "background.html",
    "permissions": [
        "https://raw.github.com/*"
    ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["meerkat.css"],
      "js": ["jquery.min.js","jqModal.js","meerkat.js"],
      "run_at":"document_start"
    }
    ]
  
}
